## user
POST          /api/user/accounts/register                        controllers.api.user.AccountController.register
GET           /api/user/accounts/:id                             controllers.api.user.AccountController.get(id: Long)
GET           /api/user/accounts                                 controllers.api.user.AccountController.list(p: daos.EntityPager ?= daos.EntityPager(), f: filters.user.AccountFilter ?= filters.user.AccountFilter(), s: daos.EntitySorter ?= daos.EntitySorter())
PUT           /api/user/accounts                                 controllers.api.user.AccountController.create
POST          /api/user/accounts/:id                             controllers.api.user.AccountController.update(id: Long)
DELETE        /api/user/accounts/:id                             controllers.api.user.AccountController.delete(id: Long)
GET           /api/user/accounts/:id/next                        controllers.api.user.AccountController.next(id: Long)
GET           /api/user/accounts/:id/prev                        controllers.api.user.AccountController.prev(id: Long)

